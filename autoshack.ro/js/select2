(function(){if(jQuery&&jQuery.fn&&jQuery.fn.select2&&jQuery.fn.select2.amd)var e=jQuery.fn.select2.amd;return e.define("select2/i18n/es",[],function(){return{errorLoading:function(){return"The results could not be loaded."},inputTooLong:function(e){var t=e.input.length- e.maximum,n="Please delete "+ t+" character";return t!=1&&(n+="s"),n},inputTooShort:function(e){var t=e.minimum- e.input.length,n="Please enter "+ t+" or more characters";return n},loadingMore:function(){return"Loading more result..."},maximumSelected:function(e){var t="You can only select "+ e.maximum+" item";return e.maximum!=1&&(t+="s"),t},noResults:function(){return"No results found"},searching:function(){return"Searching..."}}}),{define:e.define,require:e.require}})();function getCFSearchLocalization(text)
{switch(text)
{case"from":return"From";break;case"to":return"To";break;case"upload_file_to_google_drive":return"Upload file to Google Drive";break;case"very_weak":return"very weak";break;case"weak":return"weak";break;case"medium":return"medium";break;case"strong":return"strong";break;case"strength":return"Strength";break;}}